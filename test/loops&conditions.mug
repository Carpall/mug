use "shell.mug";
use "bag.mug"
func main(args: str[]): ? {
	if (args.Lenght == 0)
		panic basepanic("Give a parameter");
	# iterator
	for arg: str in args {
		io::println("arg: {0}", [arg]);
	}
	# classic
	for i: i32 to args.Lenght {
		io::println("arg{0}: {1}", [i, arg[i]]);
	}
	# while
	i32 i = 0;
	while i < args.Lenght {
		io::println("arg{0}: {1}", [i, arg[i]]);
		i++;
	}
	if args[0] == "help" {
		do();
	} elif args[0] == "h" {
		do();
	}
	else {
		do();
	}
	match args[0] {
		"help", "h" {
			io::println("Help")
		}
		else {
			io::println("fail")
		}
	}
}
func do(): ? {
	io::println("...");
}