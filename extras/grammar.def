grammar Mug;
Lexer:
   Identifier: [a-z] | [A-Z] | _
   Digit: [0-9 & .]
   Plus: '+'
   Minus: '-'
   Star: '*'
   Slash: '/'
   Colon: ':'
   ExclamationMark: '!'
   Equal: '='
   Semicolon: ';'
   OperatorEQ: '=='
   OperatorNEQ: '!='
   OpenPar: '('
   ClosePar: ')'
   OpenBracket: '['
   CloseBracket: ']'
   OpenBrace: '{'
   CloseBrace: '}'
   KeyFunc: 'func'
   KeyVar: 'var'
   KeyConst: 'const'